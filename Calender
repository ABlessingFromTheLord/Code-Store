class Entry
{
public:
	# do not change this class
	constructor(b, e)
	{
		if b >= e then throw "calendar entries must have a positive duration";
		begin = b;
		end = e;
	}
	var begin, end;
}

	var temp = [];

function arrange(entries)
{
	var result = [];
	
	# filling result with dummy values
	for var i in 0:entries.size() do{
		entries[i] = 0;
	}
	
	var min = entries[0].begin;
	var max = entries[0].end;
	var minIndex = 0;
	var maxIndex = 0;
	var i = 0;
	
	while(i < entries.size()) do{
		if (entries[i].begin < min) then {
			min = entries[i].begin;
			minIndex = i;
		}
		if (entries[i].end > max) then {
			max = entries[i].end;
			maxIndex = i;
		}
		i += 1;
	} 
	
	var difference = entries[maxIndex].end - entries[minIndex].begin;
	
	# creating column
	var column = Entry(entries[minIndex].begin, entries[maxIndex].end);
	var columnSize = difference;
	var columnCount = 0;
	
	# adding the smallest
	result[minIndex] = 0;
	difference -= (entries[minIndex].end - entries[minIndex].begin);
	
	for var i in 0:entries.size() do{
		if (difference <= entries[i].end - entries[i].begin) then{
		
			difference -= entries[i].end - entries[i].begin;
			result[i] = columnCount;
		}
		else{
			columnCount += 1;
			difference = entries[maxIndex].end - entries[minIndex].begin;
			difference -= entries[i].end - entries[i].begin;
			result[i] = columnCount;	
		}
		
	
	}

	 return result;
}

print(arrange([Entry(13, 15), Entry(11, 18), Entry(10, 12)]));

